{
  "agents": {
    "openclaw": {
      "name": "OpenClaw",
      "description": "Multi-channel AI assistant gateway with persistent memory",
      "core": {
        "perception": {
          "present": true,
          "implementation": "Multi-channel (Discord, WhatsApp, Telegram, Slack, Signal)",
          "notes": "Channel-specific parsing and normalization"
        },
        "working_memory": {
          "present": true,
          "implementation": "Session context (~130k tokens)",
          "properties": {
            "capacity": 130000,
            "ephemeral": true,
            "compaction": "automatic"
          }
        },
        "reasoning": {
          "present": true,
          "implementation": "LLM inference (Claude Opus 4.5, GPT-5.2)",
          "properties": {
            "model_agnostic": true,
            "thinking_modes": ["high", "low", "none"]
          }
        },
        "action": {
          "present": true,
          "implementation": "Tool calls, browser automation, channel actions",
          "properties": {
            "tools": ["bash", "read", "write", "browser", "skills"],
            "streaming": true
          }
        }
      },
      "extensions": {
        "episodic_memory": {
          "present": true,
          "implementation": "memory/YYYY-MM-DD.md (daily logs)",
          "properties": {
            "retention": "infinite",
            "format": "markdown"
          }
        },
        "semantic_memory": {
          "present": true,
          "implementation": "MEMORY.md + vector search",
          "properties": {
            "searchable": true,
            "embeddings": ["local", "openai", "gemini"]
          }
        },
        "procedural_memory": {
          "present": true,
          "implementation": "Skills system (ClawdHub)",
          "properties": {
            "count": 700,
            "progressive_disclosure": true
          }
        },
        "attention": {
          "present": true,
          "implementation": "requireMention, groupPolicy, DM pairing",
          "properties": {
            "filters": ["mention", "allowlist", "activeHours"]
          }
        },
        "goal_management": {
          "present": true,
          "implementation": "Loop conditions, early termination",
          "properties": {
            "bdi_lite": true
          }
        },
        "error_handling": {
          "present": true,
          "implementation": "try/catch/finally, retry with backoff",
          "properties": {
            "retry_strategies": ["exponential", "linear", "none"]
          }
        },
        "context_management": {
          "present": true,
          "implementation": "Session switching, explicit context flow",
          "properties": {
            "scopes": ["per-sender", "per-channel-peer", "main"]
          }
        },
        "meta_cognition": {
          "present": true,
          "implementation": "Heartbeat system (30min default)",
          "properties": {
            "self_monitoring": true,
            "session_reset": true
          }
        },
        "social_cognition": {
          "present": true,
          "implementation": "Multi-agent routing, coordination",
          "properties": {
            "multi_agent": true
          }
        },
        "planning": {
          "present": true,
          "implementation": "Multi-turn conversations, heartbeat scheduling",
          "properties": {
            "multi_step": true
          }
        },
        "temporal_reasoning": {
          "present": true,
          "implementation": "Cron, heartbeat scheduling",
          "properties": {
            "scheduling": true
          }
        },
        "learning": {
          "present": false,
          "notes": "Uses pre-trained LLM, no online learning"
        },
        "explanation": {
          "present": false,
          "notes": "Relies on LLM transparency, no explicit trace generation"
        },
        "emotion": {
          "present": false,
          "notes": "Not applicable for this architecture"
        },
        "embodiment": {
          "present": false,
          "notes": "Not a physical agent"
        }
      },
      "patterns": ["reactive", "deliberative", "multi-tier-memory"],
      "unique_features": [
        "Multi-channel gateway architecture",
        "Session isolation per sender/channel",
        "Progressive skill disclosure (700+ skills)",
        "Heartbeat-based self-monitoring"
      ]
    },
